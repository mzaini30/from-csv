{
  "name": "from-csv",
  "version": "0.0.1",
  "description": "Convert CSV to JSON",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "release": "bumpp --tag --commit --push",
    "build": "pkgroll",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write src/index.ts"
