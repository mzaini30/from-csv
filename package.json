  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzaini30/from-csv.git"
  },
  "keywords": [
    "github",
    "template"
  ],
  "author": "Zen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzaini30/from-csv/issues"
  },
  "homepage": "https://github.com/mzaini30/from-csv#readme",
  "devDependencies": {
    "bumpp": "^7.1.1",
    "pkgroll": "^1.3.1",
    "prettier": "^2.7.1"
  }
}
